{
    "version": 2,
    "public": true,
    "name": "showcase",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.mjs": {
            "includeFiles": "dist/showcase/**"
        }
    },
    "git": {
        "deploymentEnabled": {
            "v18": false
        }
    }
}
